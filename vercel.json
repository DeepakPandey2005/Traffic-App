{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
  { "src": "/api/posts/(.*)", "dest": "/server/index.js" },
  
      { "src": "/(.*)", "dest": "/dist/index.html" },
      { "src": "/static(.*)", "dest": "/dist/static/$1" }
    ]
  }
  