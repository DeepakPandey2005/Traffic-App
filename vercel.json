{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/assets/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
  
      { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
  
      { "src": "/(.*)", "dest": "/dist/index.html" }
    ]
  }
  