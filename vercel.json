{    "version":2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
       }
      ,
      {
        "src": "routes/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/posts/(.*)", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/dist/index.html" }
    ]
  }
  